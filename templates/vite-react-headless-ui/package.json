{
  "name": "app-name",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "rm -rf ./node_modules/.vite && unlink yarn.lock && yarn update && yarn install && yarn start",
    "update": "npx ncu -u",
    "lint": "npx eslint .",
    "fix": "npx eslint --fix -c ./.eslintrc.json ./src",
    "format": "npx prettier --write ./**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc.json",
    "start": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mongez/cache": "^1.2.2",
    "@mongez/config": "^1.0.26",
    "@mongez/dom": "^1.1.2",
    "@mongez/events": "^2.1.0",
    "@mongez/http": "^2.2.6",
    "@mongez/huskier": "^1.0.7",
    "@mongez/localization": "^2.2.4",
    "@mongez/react": "^3.0.6",
    "@mongez/react-atom": "^3.3.1",
    "@mongez/react-form": "^2.0.20",
    "@mongez/react-helmet": "^1.0.9",
    "@mongez/react-hooks": "^1.3.3",
    "@mongez/react-localization": "^1.0.5",
    "@mongez/react-router": "^2.3.0",
    "@mongez/reinforcements": "^2.3.9",
    "@mongez/supportive-is": "^2.0.3",
    "@mongez/user": "^1.0.13",
    "@mongez/validator": "^1.0.18",
    "@tabler/icons-react": "^2.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mongez/vite": "^1.0.27",
    "@types/crypto-js": "^4.1.3",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-check-updates": "^16.14.6",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-svgr": "^4.1.0"
  },
  "huskier": {
    "hooks": {
      "pre-commit": [
        "yarn fix --max-warnings 0",
        "npx tsc --noEmit"
      ]
    }
  }
}
